---
layout: null
---
{
    "tags": [
        {% for tag in site.tags %}
        "{{ tag.name }}"{% unless forloop.last %},{% endunless %}
        {% endfor %}
    ],
    "posts": [
        {% for post in site.posts %}
        {
          "{
            \"title\": {{ post.title }}, \"url\": {{ post.url }}
          }"{% unless forloop.last %},{% endunless %
        }
        {% endfor %}
    ]
}
